@model IEnumerable<sa47.team8ad.SSIS.Models.ChargeBackReportViewModel>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Charge Back Report";
}


<link href="~/Content/bootstrap.css" rel="stylesheet" />
<div class="box box-danger">
    <div class="box-header">
        <h3 class="box-title">Generate Charge-Back Report</h3>
    </div>
    <div class="box-body">
        <div>
            <div>
                <p>
                    The Charge-Back Report details the total stationery expenditure of each department for the
                    current financial year.
                </p>
            </div>
            <div>
                <iframe src="@Url.Action("ChargeBackReport", "ChargeBackReport")" height="500" width="100%"></iframe>
            </div>
        </div>
        </div>
</div>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "ViewReorderReport";
}
<link href="~/Content/bootstrap.css" rel="stylesheet" />
<div class="box box-danger">
    <div class="box-header">
        <h3 class="box-title">
            Generate Re-order Report
        </h3>
    </div>
    <div class="box-body">
        <div>

            <div>
                The Re-order Report provides the list of stationery items that have fallen below the re-order
                level this month.
            </div>
            <div>
                The report details the corresponding Purchase Order Numbers, Purchase Quantities and Expected
                Delivery Dates.
            </div>
            <div>
                <iframe src="@Url.Action("ReorderReport", "ReorderReport")" height="500" width="100%"></iframe>
            </div>
        </div>
           
        </div>
</div>